<h1><%= @song.title %></h1>


    <%= form_for (@song) do |f| %>
 <div class = "field">
    <%= f.label "title" %>
    <%= f.text_field :title %> <br>
</div>   

<div class = "field">
    label :released , "released?" %>
    <%= f.check_box :released %><br>
</div>

<div class = "field">
    <%=f.label "release year" %>
    <%= f.text_field :release_year %>
</div>

<div class = "field">
    <%= f.label :genre %>
    <%= f.text_field :genre %>
</div>

<div class = "field">
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
</div>



<% end %>